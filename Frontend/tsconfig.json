{
  "compilerOptions": {
      "target": "ES2019",
      "outDir": "./build/js",
      "rootDir": "./src",
      "strict": true,
      "skipLibCheck": true,
      "moduleResolution": "node",
      "typeRoots": ["./node_modules/@types", "./src/ts/types"],
      "lib": ["ES2019", "DOM", "DOM.Iterable"],
      "module": "ES2020",
    "baseUrl":".",
    "paths": {
      "@shared/*":["src/Shared/*"]
    }
  },
  "include": ["src/**/*", "src/Shared/**/*"]
}
